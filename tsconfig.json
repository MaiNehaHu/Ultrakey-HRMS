{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)",
    "app/login",
    "app/forgotPassword",
    "app/leaveDetails",
    "app/applyLeave",
    "app/punchDetails",
    "app/taskDetails",
    "app/morePage",
    "app/holidaysList",
  ]
}
